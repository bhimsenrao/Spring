<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<bean id="address" class="com.spr.Address">
<property name="street" value="Mamatha nagar"/>
<property name="doorno" value="23-33"/>
<property name="area" value="Nagole"/>
</bean>

<bean id="person" class="com.spr.Person" autowire="byType">
<property name="pname" value="Rajendra"/>
</bean>

</beans>