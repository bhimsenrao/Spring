<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<context:annotation-config />
	<bean id="Java" class="com.wire.Technology" >
	<property name="techname" value="JavaEE"/>
	<property name="experience" value="10"/>
	</bean>

	<bean id="Angular" class="com.wire.Technology">
		<property name="techname" value="Angular7" />
		<property name="experience" value="4"/>
	</bean>
	<bean id="dev" class="com.wire.Developer" />
</beans>